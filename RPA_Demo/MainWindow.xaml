<Window x:Class="RPA_Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:app="clr-namespace:RPA_Demo.View"
        xmlns:local="clr-namespace:RPA_Demo"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="600" Width="920"
        WindowStyle="None"
        ResizeMode="NoResize"
        Background="Transparent"
        AllowsTransparency="True">
    <Border Background="#fceded"
            CornerRadius="10"
            Name="Main">
        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="915" />
            </Grid.ColumnDefinitions>
            <Border Background="#242424"
                    CornerRadius="5"
                    Margin="5">
                <WrapPanel Name="loadView" HorizontalAlignment="Center" VerticalAlignment="Center"></WrapPanel>
            </Border>
        </Grid>
    </Border>
</Window>
